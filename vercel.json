{
  "version": 2,
  "name": "softedubackmongodb",
  "builds": [
    { 
      "src": "server.js", 
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "/server.js" 
    }
  ],
  "headers": [
    {
      "source": "/auth/login",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://fichas-tecnicas-lastra.vercel.app" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ]
}
